  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.5.0",
    "bumpp": "^9.1.1",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "kolorist": "^1.8.0"
