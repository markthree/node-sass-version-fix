{
  "version": "0.0.0",
  "name": "node-sass-version-fix",
  "description": "fix node-sass version",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "node-sass-version-fix": "dist/cli.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markthree/node-sass-version-fix.git"
  },
  "exports": {
    ".": {
